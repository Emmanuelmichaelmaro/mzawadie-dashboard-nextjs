    "scripts": {
        "dev": "next dev -p 9000",
        "build": "next build",
        "start": "next start -p 9000",
        "lint": "next lint",
        "eslint": "eslint \"src/**/*.ts*\" -c ./.eslintrc --quiet --fix",
        "format": "prettier --write \"**/*.+(ts|tsx|js|jsx|json|yml|yaml|css|md|html)\"",
        "check-types": "tsc --noEmit",
        "graphql-schema-download": "apollo schema:download --endpoint=https://demo.saleor.io/graphql/ schema.graphql",
        "graphql-build-types": "apollo client:codegen --target=typescript --tagName=gql --globalTypesFile=src/types/globalTypes.ts types",
        "graphql-codegen-generate": "graphql-codegen --config codegen.yml --watch",
        "storybook": "start-storybook -p 5000",
        "build-storybook": "build-storybook -o build/storybook",
        "extract": "formatjs extract",
        "extract-messages": "npm run extract -- \"src/**/!(*.{d,test}).ts*\" --out-file locale/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
        "compile": "formatjs compile",
        "compile-messages": "npm run compile -- locale/en.json --ast --out-file locale/compiled-locales/en.json",
        "react-intl-extract-compiled-messages": "npm run extract-messages && npm run compile-messages",
        "react-intl-extract-messages": "cross-env NODE_ENV=development extract-messages -l=en,sw -o locale/translations -d en --flat false 'src/**/!(*.{d,test}).{ts,tsx}'"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run check-types"
        }
    },
    "lint-staged": {
        "*.+(ts|tsx|js|jsx)": [
            "eslint 'src/**/*.{ts,tsx,js,jsx}' --quiet --fix",
            "git add"
        ],
        "*.+(json|css|md)": [
            "prettier --write",
            "git add"
        ]
    },
