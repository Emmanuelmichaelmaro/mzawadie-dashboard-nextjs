    "scripts": {
        "start": "webpack-dev-server -d",
        "build": "webpack -p",
        "postbuild": "rimraf ./build/**/*.js.map",
        "postinstall": "patch-package",
        "prepare": "husky install && husky add .husky/pre-commit \"node_modules/.bin/lint-staged\" && husky add .husky/pre-push \"npm run check-types\"",
        "lint": "eslint \"src/**/*.@(tsx|ts|jsx|js)\" -c ./.eslintrc --quiet --fix",
        "format": "prettier --write \"**/*.+(ts|tsx|js|jsx|json|yml|yaml|css|scss)\"",
        "test": "TZ=UTC jest src/",
        "test:watch": "jest --watchAll --verbose",
        "test:coverage": "jest --coverage",
        "cypress": "cypress open",
        "cypress:run": "cypress run",
        "cypress:record": "npm run cypress:run -- --record",
        "e2e": "start-server-and-test dev http://localhost:9000 cypress",
        "e2e:run": "start-server-and-test dev http://localhost:9000 cypress:run",
        "e2e:record": "start-server-and-test dev http://localhost:9000 cypress:record",
        "check-strict-null-errors": "tsc --noEmit --strictNullChecks | node scripts/count-strict-null-check-errors.js",
        "check-types": "tsc --noEmit",
        "graphql-schema-download": "apollo schema:download --endpoint=https://localhost:8000/graphql/ schema.graphql",
        "graphql-build-types": "apollo client:codegen --target=typescript --tagName=gql --globalTypesFile=src/types/globalTypes.ts types",
        "graphql-codegen-generate": "graphql-codegen --config codegen.yml",
        "extract": "formatjs extract",
        "extract-messages": "npm run extract -- \"src/**/!(*.{d,test}).ts*\" --out-file locale/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
        "compile": "formatjs compile",
        "compile-messages": "npm run compile -- locale/es.json --ast --out-file locale/compiled-locales/en.json",
        "formatjs-extract-compile-messages": "npm run extract-messages && npm run compile-messages",
        "extract-json-messages": "rimraf build/locale && cross-env NODE_ENV=extract babel src 'src/**/*.{ts,tsx}' -o build/dashboard.bundle.js",
        "transpile-messages": "node scripts/transpile-tx.js",
        "react-intl-extract-transpile-messages": "npm run extract-json-messages && npm run transpile-messages",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "release": "release-it"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint -c ./.eslintrc --fix",
            "prettier --write"
        ],
        "*.{js,jsx}": [
            "eslint -c ./.eslintrc --fix",
            "prettier --write"
        ]
    }
}
