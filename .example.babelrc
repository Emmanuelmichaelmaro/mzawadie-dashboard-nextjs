{
    "presets": ["next/babel"],
    "plugins": [
        "@babel/plugin-proposal-do-expressions",
        [
            "react-intl",
            {
                "messagesDir": "./build/messages",
                "extractSourceLocation": true
            }
        ],
        [
            "formatjs",
            {
                "idInterpolationPattern": "[sha512:contenthash:base64:6]",
                "ast": true
            }
        ]
    ]
}
